<h2>メンバーのページ</h2>
<table>
  <thead>
    <tr>
      <th>名前</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @team.grouping_users.each do |groupings| %>
      <tr>
        <td><%= groupings.name %></td>
        <td><%= groupings.email %></td>
        <td><%#= link_to "削除する", task_path(task.id), method: :delete, class: "btn btn-danger" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "メインメニューへ", menus_path, class: "btn btn-light" %>

