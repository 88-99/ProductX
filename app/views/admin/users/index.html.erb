<h2>【管理者用】ユーザ一覧</h2>
<table>
<thead>
  <tr>
    <th>ユーザID</th>
    <th>ユーザネーム</th>
    <th>Email</th>
    <th>電話番号</th>
    <th>ニックネーム</th>
    <th>admin</th>
  </tr>
</thead>
<tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.phone_number %></td>
      <td><%= user.nickname %></td>
      <td><%= user.admin %></td>
      <td><%= link_to "ユーザ削除", admin_user_path(user.id), method: :delete, class: "btn btn-danger" %></td>
    </tr>
  <% end %>
</tbody>
</table>