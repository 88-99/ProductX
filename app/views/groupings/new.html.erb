<h4>メンバー登録</h4>
<%= form_with(model: @grouping, url: team_groupings_path(params[:team_id]), local: true) do |form| %>
  <% if @grouping.errors.any? %>
    <div id="error_explanation">
      <h4><%= @grouping.errors.count %>件のエラーがあります。</h4>
      <ul>
      <% @grouping.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>  
  <div class="grouping_email">
    <%= form.label :メンバーのemail %>
    <%= form.text_field :email, placeholder: "メンバーのemail" %>
  </div>
  
  <%= form.submit %>
<% end %>
<%= link_to 'メンバー一覧画面', team_path(current_user.team) %>
<%= link_to 'メインメニュー', menus_path %>